<h1>Welcome</h1>
<p>This is a basic chat app</p>
  <%= @rooms.each do |r| %>
    <div class="card-block">
      <%= r.name %> 
    </div>
  <% end %>

  <%= form_for new_room_path do |f| %>
  <div class="field" style="display:inline-block">
    <%= f.text_field :name, placeholder: 'Room name', class: 'form-control' %>
  </div>
  <div class="actions" style="display:inline-block">
    <%= f.submit "Create", class: 'btn btn-success' %>
  </div>

  <% end %>

